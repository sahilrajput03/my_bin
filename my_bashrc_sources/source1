# LEARN: cd doesn't work subshell scripst, so remember you get to use only aliases for `cding`.
alias sp='cd /c/Users/chetan/AppData/Roaming/code/User/snippets/' # TODO: change path for linux.
alias cdflexzombies='cd /home/array/my_bin/snips/css/flexbox-zombies'
alias fishfunctions='cd /usr/share/fish/functions/'
alias fishbashrc_cd='cd ~/.config/fish/'

alias fishbashrc_source='. ~/.config/fish/config.fish'

export PATH="/home/array/my_bin:/home/array/my_bin/aliases:$PATH"
# ^^ This would not work for any sudo prefixed command and will rather throw error that binary not found, so you need to edit visudo /etc/sudoers file or so, check in your "Linux walkthrough file."

set NODE_PATH /usr/lib/node_modules
